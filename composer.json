{
    "name": "beedelivery/laravel-itpay",
    "description": "Package for integration with ItPay.",
    "keywords": ["itpay", "laravel", "bank"],
    "type": "library",
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*",
        "Illuminate/Support": "^8.0",
        "Illuminate/http": "^8.21",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/validation": "^8.0"
    },
    "license": "MIT",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Bee Delivery",
            "email": "suporte@beedelivery.com.br"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "BeeDelivery\\ItPay\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeeDelivery\\ItPay\\ItPayServiceProvider"
            ],
            "aliases": {
                "ItPay": "BeeDelivery\\ItPay\\Facades\\ItPay"
            }
        }
    }
}
